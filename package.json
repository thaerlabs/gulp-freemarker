{
  "name": "gulp-freemarker",
  "version": "0.1.3",
  "description": "A plugin for Gulp to process Freemarker template files like Java.",
  "keywords": [
    "gulpplugin",
    "freemarker",
    "freemarker.js",
    "fmpp"
  ],
  "author": {
    "name": "thaerlabs",
    "email": "abbas.t249@gmail.com",
    "url": "https://github.com/thaerlabs"
  },
  "repository": "thaerlabs/gulp-freemarker",
  "scripts": {
    "test": "istanbul test ./node_modules/mocha/bin/_mocha --report html -- test/*.js --reporter spec",
