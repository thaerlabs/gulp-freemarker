  },
  "dependencies": {
    "freemarker.js": "https://github.com/thaerlabs/freemarker.js/tarball/a24a3b9ea271d7075eabf18106dd873aea990afd",
    "gulp-util": "^3.0.4",
    "through2": "^1.1.1"
  },
  "devDependencies": {
    "coveralls": "*",
    "event-stream": "*",
    "istanbul": "*",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "should": "^5.2.0"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
